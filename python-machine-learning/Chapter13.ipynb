   "source": [
    "import theano\n",
    "from theano import tensor as T\n",
    "# 初期化：scalerメソッドではスカラー（単純な配列）を生成\n",
    "x1 = T.scalar()\n",
    "w1 = T.scalar()\n",
    "w0 = T.scalar()\n",
    "z1 = w1 * x1 + w0\n",
    "# コンパイル\n",
    "net_input = theano.function(inputs=[w1,x1,w0],outputs=z1)\n",
    "# 実行\n",
    "print('Net input: %.2f' % net_input(2.0, 1.0, 0.5))"
   ]
